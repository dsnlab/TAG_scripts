#!/usr/bin/env bash


###################################################################
#  ☭  ☭  ☭  ☭  ☭  ☭  ☭  ☭  ☭  ☭  ☭  ☭  ☭  ☭  ☭  ☭  ☭  ☭  ☭  ☭  ☭  #
###################################################################

###################################################################
# Get subject identifiers
###################################################################

unset id_var id_val
i=1
for f in "${subject[@]}"
   do
   if [[ ! -e ${f} ]]
      then
      id_var="${id_var} subject[${i}]"
      id_val="${id_val} ${f}"
   fi
   (( i++ ))
done
id_var=( ${id_var} )
id_val=( ${id_val} )
