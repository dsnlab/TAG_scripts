#!/usr/bin/env bash

###################################################################
#  ☭  ☭  ☭  ☭  ☭  ☭  ☭  ☭  ☭  ☭  ☭  ☭  ☭  ☭  ☭  ☭  ☭  ☭  ☭  ☭  ☭  #
###################################################################

###################################################################
# Assign any directly declared variables.
###################################################################
for a in "${asgt[@]}"
   do
   contains ${a} '^-' && continue
   a_var=$(strslice ${a} 1 =)
   a_val=$(strslice ${a} 2 =)
   eval ${a_var}=\${a_val}
done
